#include <iostream>
#include <iomanip> 
using namespace std;

int main() {
	int T; cin >> T;
	cout << setiosflags(ios::uppercase);
	cout << setw(0xf) << internal;
	while(T--) {
		double A; cin >> A;
		double B; cin >> B;
		double C; cin >> C;
        cout << left << hex << showbase << nouppercase << long(A) << endl;
        cout << setprecision(2) << setw(15) << right << setfill('_') << showpos << fixed << B << endl;
        cout<< setprecision(9) << uppercase << scientific << noshowpos << C << endl;
	}
	return 0;
}
